<antlib>

  <typedef name="authentication"       classname="org.sonatype.aether.ant.types.Authentication"/>
  <typedef name="proxy"                classname="org.sonatype.aether.ant.types.Proxy"/>
  <typedef name="mirror"               classname="org.sonatype.aether.ant.types.Mirror"/>
  <typedef name="localrepo"            classname="org.sonatype.aether.ant.types.LocalRepository"/>
  <typedef name="remoterepo"           classname="org.sonatype.aether.ant.types.RemoteRepository"/>
  <typedef name="remoterepos"          classname="org.sonatype.aether.ant.types.RemoteRepositories"/>
  <typedef name="dependency"           classname="org.sonatype.aether.ant.types.Dependency"/>
  <typedef name="dependencies"         classname="org.sonatype.aether.ant.types.Dependencies"/>
  <typedef name="artifact"             classname="org.sonatype.aether.ant.types.Artifact"/>
  <typedef name="artifacts"            classname="org.sonatype.aether.ant.types.Artifacts"/>
  <typedef name="settings"             classname="org.sonatype.aether.ant.types.Settings"/>

  <taskdef name="resolve"              classname="org.sonatype.aether.ant.tasks.Resolve"/>
  <taskdef name="install"              classname="org.sonatype.aether.ant.tasks.Install"/>
  <taskdef name="deploy"               classname="org.sonatype.aether.ant.tasks.Deploy"/>
  <taskdef name="pom"                  classname="org.sonatype.aether.ant.types.Pom"/>

</antlib>
